{
    "name": "dulabs/iak-api-php",
    "description": "PHP-based SDK for IAK API Services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Indobest Artha Kreasi",
            "email": "techsupport@mobilepulsa.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dulabs/iak-api-php.git"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Dulabs\\IakApiPHP\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0 || ^8.0",
        "guzzlehttp/guzzle": "^6.5 || ^7.0",
        "vlucas/phpdotenv": "^4.2 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.5",
        "mockery/mockery": "~1.3"
    }
}
